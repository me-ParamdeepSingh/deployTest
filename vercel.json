{
    "builds": [
        {
            "src": "Restoran/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxlambdaSize": "15mb", "runtime": "python3.12.0" }
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config": { 
                "disDir": "staticfiles_build"
             }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "Restoran/wsgi.py"
        }
    ]
}